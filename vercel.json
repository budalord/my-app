{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/main.py" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/build/index.html" }
  ],
  "env": {
    "NODE_VERSION": "14"
  }
}
